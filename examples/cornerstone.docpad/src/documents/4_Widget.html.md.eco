        <h1>Heading</h1>
        <p>Tagline</p>
        <p>
            <a class="btn btn-primary btn-large">
            Learn more
            </a>
        </p>
    </div>
<% end %><%- codeMirrorBlock {func : prv_46 , funcname : 'prv_46'} %>

